        SecondParagraph: 
        formatUI: {fileID: 0}
    - contentType: 2
      customContent:
        playerQuestion: 
        formatUI: {fileID: 0}
      anserWithTextContent:
        playerQuestion: Mengapa menanam itu penting?
        NpcAnswer: Teman-teman bisa masuk ke dalam rumah kaca di belakang dan mulai
          mengerjakan misi untuk belajar menanam!
        formatUI: {fileID: 3022390831953900334, guid: 8aab68401856780469e263cd1c1b6573,
          type: 3}
      anserWithTextAndPhotoContent:
        playerQuestion: Kapan tanaman harus disiram atau dipupuk?
        illustarationImage: {fileID: 21300000, guid: e0c589d584f7ecc41ad2231681457557,
          type: 3}
        firstParagraph: Garis hijau menunjukkan kebutuhan tanaman akan air/pupuk.
          Jika garis belum penuh, tanaman masih butuh bahan tersebut untuk naik level.
        SecondParagraph: Garis hijau menunjukkan kebutuhan tanaman akan air/pupuk.
